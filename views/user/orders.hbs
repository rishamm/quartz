      <section class="cart_area section_padding">
        <div class="container">
          <div class="cart_inner">
            <div class="table-responsive">
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Products</th>
                    <th scope="col">Price</th>
                    <th scope="col">Quantity</th>
                    <th scope="col">Total</th>
                     <th scope="col">Status</th>
                      <th style="width:500; "scope="col">Cancel</th>
                <th></th>
                  </tr>
                 
                </thead>
                   {{#each ordrs}}
                <tbody class="mt-5">
                  
                  <tr>
                    <td>
                      <div class="media">
                        <div class="d-flex">
                          <img src="/images/{{this.orders.productss.cart.productdetails.products.Images.[0].filename}}" alt="" />
                        </div>
                        <div class="media-body">
                          <p></p>
                        </div>
                      </div>
                    </td>
                    <td>
                      <h5>Rs.{{this.orders.productss.cart.productdetails.products.price}}</h5>
                    </td>
                
                    <td><h5>{{this.orders.productss.cart.qty}}</h5></td>
                    <td><h5>Rs.{{this.orders.productss.cart.price}}</h5></td>
                    <td  id="status">{{this.orders.productss.cart.status}}</td>
                    <td>  <button  class="btn_3 btn-danger" role="button"
                                        data-bs-toggle="button" onclick="cancel('{{this.orders.productss.cart._id}}','{{this.orders._id}}')" data-toggle="modal" data-target="#exampleModal{{this._id}}">cancel</button></td>
                  </tr>
                
                 
             {{/each}}
               
                </tbody>
              </table>
           
